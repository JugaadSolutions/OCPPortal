@section menu{
    @Html.Partial("Menu")
}

@section scripts{
    <script type="text/javascript">
        $('#ReportDateTimePickerDiv').jqxDateTimeInput({ width: '100%', formatString: 'dd/MM/yyyy', showTimeButton: false, value: null });
            $('#ReportEndDateTimePickerDiv').jqxDateTimeInput({ width: '100%', formatString: 'dd/MM/yyyy', showTimeButton: false, value: null });
            </script>
    }
<div class="container">
    <div class="row">
        <div class="col-md-2" style="margin-top:2%;text-align:left;margin-left:20%;padding-top:1%">
           
                <label id="operatorId">Order Number:</label>
            
        </div>
        <div class="col-md-2" style="margin-top:2%;text-align:right;margin-left:-7%">
           
                <input class=" form-control input-md" id="Operator" type="text">
            
        </div>
        <div class="col-md-3" style="margin-top:2%;padding-top:0%">
           
                <button id="ShowButton" class="btn btn-info btn-lg ">
                    Show
                </button>
           
        </div>
    </div>
    <div class="row" style="margin-top:2%;">
        <div class="col-md-3">
           
                <b> From:</b>
           
        </div>
        <div class="col-md-3" style="">
          
                <div id="ReportDateTimePickerDiv" style="margin-left:-90%">

                </div>
               
            </div>
        <div class="col-md-3" style="margin-left:-20%">
          
                <b>To:</b>
            
        </div>

        <div class="col-md-3" style="">
          
                <div id="ReportEndDateTimePickerDiv" style="margin-left:-90%;">


                </div>
               
            </div>
    </div>
    <div class="row">
        <div class="col-md-10">
            <div class="col-md-offset-4" style="margin-top:2%;">
                <button id="ShowBtn" class="btn btn-info btn-lg ">
                    Show
                </button>
            </div>
        </div>
    </div>
</div>